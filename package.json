{
  "name": "@org/source",
  "version": "2.1.2",
  "license": "MIT",
  "scripts": {
    "release": "release-it",
    "prepare": "npx husky && lint-staged --concurrent false"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx lint-staged --concurrent false",
      "pre-push": "npx lint-staged -concurrent false"
    }
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: v${version}"
    },
    "github": {
      "release": false
    },
    "npm": {
      "publish": false
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "infile": "apps/site/src/assets/md/CHANGELOG.md",
        "preset": {
          "name": "conventionalcommits",
          "typws": [
            {
              "type": "feat",
              "section": "features"
            },
            {
              "type": "fix",
              "section": "fixes"
            }
          ]
        }
      }
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix \"**/*.ts\"",
      "prettier * --write",
      "git commit . -F --allow-empty"
    ],
    "*.html": [
      "prettier --write",
      "git commit . -F --alow-empty"
    ],
    "*.scss": [
      "prettier --write",
      "stylelint \\\"**/*.scss\\\" --fix -f",
      "git commit --allow-empty .  -F "
    ]
  },
  "private": true,
  "dependencies": {
    "@angular-eslint/schematics": "^17.3.0",
    "@angular-eslint/utils": "^17.3.0",
    "@angular/animations": "17.3.2",
    "@angular/common": "17.3.2",
    "@angular/compiler": "17.3.2",
    "@angular/core": "17.3.2",
    "@angular/forms": "17.3.2",
    "@angular/platform-browser": "17.3.2",
    "@angular/platform-browser-dynamic": "17.3.2",
    "@angular/router": "17.3.2",
    "@floating-ui/dom": "^1.6.4",
    "@ng-web-apis/common": "^3.0.6",
    "@nx/angular": "18.3.4",
    "@release-it/conventional-changelog": "^8.0.1",
    "@tinkoff/ng-polymorpheus": "^4.3.0",
    "@types/prismjs": "^1.26.3",
    "marked": "^9.1.6",
    "ngx-markdown": "^17.1.1",
    "prism-themes": "^1.3.0",
    "release-it": "^17.2.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "17.3.6",
    "@angular-devkit/core": "17.3.6",
    "@angular-devkit/schematics": "17.3.6",
    "@angular-eslint/eslint-plugin": "17.3.0",
    "@angular-eslint/eslint-plugin-template": "17.3.0",
    "@angular-eslint/template-parser": "17.3.0",
    "@angular/cli": "17.3.6",
    "@angular/compiler-cli": "17.3.2",
    "@angular/language-service": "17.3.2",
    "@antfu/eslint-config": "^2.9.0",
    "@commitlint/config-conventional": "^19.1.0",
    "@nx/devkit": "18.3.4",
    "@nx/eslint": "18.3.4",
    "@nx/eslint-plugin": "18.3.4",
    "@nx/jest": "18.3.4",
    "@nx/js": "18.3.4",
    "@nx/playwright": "18.3.4",
    "@nx/workspace": "18.3.4",
    "@playwright/test": "^1.36.0",
    "@schematics/angular": "17.3.2",
    "@stylistic/eslint-plugin": "^1.7.0",
    "@swc-node/register": "~1.8.0",
    "@swc/core": "~1.3.85",
    "@swc/helpers": "~0.5.2",
    "@types/date-fns": "^2.6.0",
    "@types/jest": "^29.4.0",
    "@types/node": "18.16.9",
    "@typescript-eslint/eslint-plugin": "7.4.0",
    "@typescript-eslint/parser": "7.4.0",
    "date-fns": "^3.6.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-playwright": "^0.15.3",
    "eslint-plugin-regex": "^1.10.0",
    "eslint-plugin-unicorn": "^51.0.1",
    "eslint-plugin-unused-imports": "^3.1.0",
    "husky": "^9.0.11",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.4.1",
    "jest-preset-angular": "~14.0.3",
    "lint-staged": "^15.2.2",
    "nx": "18.3.4",
    "prettier": "^2.6.2",
    "stylelint": "^14.1.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-order": "^5.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "typescript": "5.4.3"
  }
}
